.taskdetial-row {
  display: flex;
  flex-direction: row;
  align-items: flex;
  margin: 20rpx;
}

.taskdetial-column {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.taskdetial-img {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.taskdetial-text {
  color: #aaa;
  margin: 20rpx;
}

.taskdetial-time {
  color: rgb(54, 34, 34);
  margin: 20rpx;
}