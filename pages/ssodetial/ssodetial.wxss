.ssodetial-row {
  display: flex;
  flex-direction: row;
  align-items: flex;
  margin: 20rpx;
}

.ssodetial-column {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.ssodetial-text {
  color: #aaa;
  margin: 20rpx;
}

.ssodetial-img {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.ssodetial-time {
  color: rgb(54, 34, 34);
  margin: 20rpx;
}
