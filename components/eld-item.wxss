.eld-row {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.eld-column {
  display: flex;
  flex-direction: column;
  align-items: left;
}

.eld-icon {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.eld-name {
  color: #aaa;
}

.eld-age {
  color: #aaa;
  margin: 10rpx;
  font-size: 12px;
}

.eld-gender {
  color: #aaa;
  margin: 10rpx;
  font-size: 12px;
}
